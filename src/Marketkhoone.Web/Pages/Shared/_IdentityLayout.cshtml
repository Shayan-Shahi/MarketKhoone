<!doctype html>
<html lang="fa" dir="rtl">

<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link rel="stylesheet" href="~/lib/bootstrap/css/bootstrap.rtl.min.css" />
    <link rel="stylesheet" href="~/lib/bootstrap-icons/font/bootstrap-icons.min.css">

    <!---Webiste Stlyes --->
    <link rel="stylesheet" href="~/websiteStyles/css/vendor/font-awesome.min.css">
    <link rel="stylesheet" href="~/websiteStyles/css/vendor/materialdesignicons.css">
    <!-- plugin-------------------------------------->
    <link rel="stylesheet" href="~/websiteStyles/css/vendor/bootstrap.css">
    <link rel="stylesheet" href="~/websiteStyles/css/vendor/owl.carousel.min.css">
    <link rel="stylesheet" href="~/websiteStyles/css/vendor/noUISlider.min.css">
    <!-- main-style---------------------------------->
    <link rel="stylesheet" href="~/websiteStyles/css/main.css">
    
    <!--با استفاده از این تگ نو اسکریپت، از ورود کاربرانی که جاوا اسکریپت مرورگرشان را غیرفعال کرده اند جلوگیری میکنیم-->
    <noscript>
        <meta http-equiv="refresh" content="0;url=/NoScripts.html" />
    </noscript>
    @await RenderSectionAsync("Styles", false)
    <link rel="stylesheet" href="~/css/site.css">
    <link rel="stylesheet" href="~/MarketKhoone.Web.styles.css">
    <title>@ViewData["Title"] | کالا مارکت</title>
</head>

<body>

    @RenderBody()


    <!-- file js---------------------------------------------------->
    <script src="~/websiteStyles/js/vendor/jquery-3.2.1.min.js"></script>
    <script src="~/websiteStyles/js/vendor/bootstrap.js"></script>
    <!-- plugin----------------------------------------------------->
    <script src="~/websiteStyles/js/vendor/owl.carousel.min.js"></script>
    <script src="~/websiteStyles/js/vendor/jquery.countdown.js"></script>
    <script src="~/websiteStyles/js/vendor/ResizeSensor.min.js"></script>
    <script src="~/websiteStyles/js/vendor/theia-sticky-sidebar.min.js"></script>
    <script src="~/websiteStyles/js/vendor/wNumb.js"></script>
    <script src="~/websiteStyles/js/vendor/nouislider.min.js"></script>
    <!-- main js---------------------------------------------------->
    <script src="~/websiteStyles/js/main.js"></script>

    <script src="~/lib/jquery/jquery.min.js"></script>
    <script src="~/lib/bootstrap/js/bootstrap.bundle.min.js"></script>
    @await RenderSectionAsync("Scripts", false)
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("CustomScripts", false)
</body>

</html>